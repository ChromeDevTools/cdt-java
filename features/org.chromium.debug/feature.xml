<feature
      id="org.chromium.debug"
      label="%featureName"
      version="0.3.9.qualifier"
      provider-name="%providerName"
      plugin="org.chromium.debug">

   <description url="http://code.google.com/p/chromedevtools/wiki/EclipseDebugger">
      %description
   </description>

   <copyright>
      Copyright (c) 2012 The Chromium Authors. All rights reserved.
   </copyright>

   <license url="%licenseUrl">
      %licenseAgreement
   </license>

   <url>
      <update label="ChromeDevTools Update Site" url="http://chromedevtools.googlecode.com/svn/update/dev/"/>
   </url>

   <requires>
      <import feature="org.chromium.sdk" version="0.3.9" match="compatible"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.filesystem" version="1.2.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.debug.ui" version="3.4.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui" version="3.4.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jface.text" version="3.4.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.workbench.texteditor" version="3.4.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.editors" version="3.4.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.variables" version="3.2.100" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.ide" version="3.4.1" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.chromium.debug.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.chromium.debug.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>
   <plugin
         id="org.chromium.debug"
         download-size="0"
         install-size="0"
         version="0.0.0"/>

</feature>
